    type: &#39;&#39;,
    customType: {
          m3u8: playM3u8,
          flv: playFlv,
      },
    autoplay: false,
    pip: true,
    setting: true,
    aspectRatio: true,
    fullscreen: true,
    fullscreenWeb: true,
    subtitleOffset: true,
});
&lt;/script&gt;</description>
      
